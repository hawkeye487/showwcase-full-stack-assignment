generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id     Int    @id @default(autoincrement())
  userId String @unique
  name   String
}
model Education {
  id           Int     @id @default(autoincrement())
  userId       String
  school       String
  degree       String
  fieldOfStudy String
  startMonth   String
  startYear    String
  endMonth     String
  endYear      String
  description  String?
}
