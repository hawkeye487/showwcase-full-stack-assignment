generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Education {
  id          Int       @id @default(autoincrement())
  userId      String    
  school      String
  degree      String
  fieldOfStudy String
  startDate   String
  endDate     String?
  description String?
}